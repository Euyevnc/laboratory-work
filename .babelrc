{
  "presets": [["@babel/preset-env", { "targets": "defaults" }]],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": true,
        "version": "7.0.0-beta.0"
      }
    ],
    ["@babel/plugin-proposal-class-properties"]
  ]
}